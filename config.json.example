{
  "server": {
    "host": "127.0.0.1",
    "port": 9002,
    "read_timeout": 15,
    "write_timeout": 15
  },
  "entry_point": "https://api.apiproxy.app",
  "api_key": "apx_live_your_api_key_here",
  "cache": {
    "backend": "sqlite",
    "path": "~/.apiproxy/cache.db",
    "ttl": 86400,
    "postgres_dsn": ""
  },
  "plugins": {
    "enabled": false,
    "plugins": [
      {
        "name": "logger",
        "type": "go",
        "path": "~/.apiproxy/plugins/go/logger.so",
        "enabled": true,
        "config": {}
      },
      {
        "name": "custom_router",
        "type": "go",
        "path": "~/.apiproxy/plugins/go/custom_router.so",
        "enabled": true,
        "config": {
          "routes": {
            "/v1/stripe/*": "https://api.stripe.com",
            "/v1/twilio/*": "https://api.twilio.com",
            "/v1/openai/*": "https://api.openai.com"
          }
        }
      },
      {
        "name": "python_logger",
        "type": "python",
        "path": "~/.apiproxy/plugins/python/logger.py",
        "enabled": false,
        "config": {}
      },
      {
        "name": "openai_adapter",
        "type": "python",
        "path": "~/.apiproxy/plugins/python/openai_adapter.py",
        "enabled": false,
        "config": {
          "openai_api_key": "sk-your-openai-key-here"
        }
      }
    ]
  },
  "offline_endpoints": [
    "/v1/darkapi/ip/*",
    "/v1/nerdapi/hash",
    "/v1/computeapi/*",
    "/health",
    "/status"
  ],
  "whitelisted_endpoints": [
    "/v1/darkapi/*",
    "/v1/nerdapi/*",
    "/v1/computeapi/*",
    "/v1/systemapi/*",
    "/v1/ipapi/*",
    "/v1/stripe/*",
    "/v1/twilio/*",
    "/v1/openai/*",
    "/v1/custom/*"
  ]
}
